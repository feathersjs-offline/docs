{
  "title": "FeathersJS",
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "feathers-collapsible-menu",
    "heading-anchors",
    "include-codeblock",
    "edit-link",
    "ga",
    "feathers-versions",
    "advanced-emoji",
    "codetabs"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/feathersjs/docs/edit/master",
      "label": "Edit This Page",
      "placement": "left"
    },
    "ga": {
      "token": "UA-64904930-1"
    },
    "lunr": {
      "maxIndexSize": 1000000000
    },
    "menu": {
      "deep": true
    },
    "versions": {
      "gitbookConfigURL": "https://rawgit.com/feathersjs/feathers-docs/master/book.json",
      "options": [
        {
          "value": "https://crow.docs.feathersjs.com",
          "text": "v4 (Crow)"
        },
        {
          "value": "https://docs.feathersjs.com",
          "text": "v3 (Buzzard)"
        },
        {
          "value": "https://auk.docs.feathersjs.com",
          "text": "v2 (Auk)"
        },
        {
          "value": "https://legacy.docs.feathersjs.com",
          "text": "v1 (Legacy)"
        }
      ]
    }
  }
}